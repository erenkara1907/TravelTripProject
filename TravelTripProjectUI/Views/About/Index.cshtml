@using TravelTripProjectUI.Models.Classes
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<!DOCTYPE HTML>
<html>
<body>
    <div class="content">
        <div class="wrap">
            <div class="about">
                <h3>Hakkımızda</h3>
                @foreach (var x in Model)
                {
                    <img src="@x.PhotoUrl" alt="" />

                    <p>@x.Description</p>
                }
            </div>
        </div>
    </div>
</body>
</html>

